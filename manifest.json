{
  "manifest_version": 2,
  "name": "Blur Images",
  "version": "1.0",
  "description": "A Firefox addon that blurs all images.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "css": ["styles.css"],
      "run_at": "document_start"
    }
  ]
}